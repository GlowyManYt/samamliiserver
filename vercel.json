{
  "version": 2,
  "builds": [
    {
      "src": "dist/vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/vercel.js"
    },
    {
      "src": "/health",
      "dest": "/dist/vercel.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/dist/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  },
  "functions": {
    "dist/vercel.js": {
      "maxDuration": 30
    }
  }
}
